<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:Trace"
    xmlns:controls="clr-namespace:Trace.Views.Controls">
    
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/Trace;component/Views/Controls/BaseWindow.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <Style BasedOn="{StaticResource BaseWindowStyle}" TargetType="{x:Type controls:BaseWindow}" />
</ResourceDictionary>
